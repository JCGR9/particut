{
  "name": "particut",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Professional PDF score processing tool for automated printing and guillotining preparation",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "vite preview --port 4173",
    "clean": "rm -rf dist",
    "clean:deps": "rm -rf node_modules package-lock.json",
    "reinstall": "npm run clean:deps && npm install",
    "check": "npx browserslist@latest --update-db",
    "audit": "npm audit --audit-level moderate",
    "start": "npm run dev"
  },
  "devDependencies": {
    "vite": "^7.1.6"
  },
  "dependencies": {
    "pdfjs-dist": "^4.0.0",
    "pdf-lib": "^1.17.1",
    "jspdf": "^2.5.2"
  }
}
